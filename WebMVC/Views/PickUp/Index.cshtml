@{
    Layout = "_PickUpLayout";
}

@model PickupListViewModel
@foreach (var delivery in Model.Deliveries)
{
    <partial name="DeliverySummary" model="delivery" />
}

<div page-model-deliveries="@Model.PagingInfo" page-action="Index" page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-secondary"
     page-class-selected="btn-primary" page-url-category="@Model.CurrentCategory"
     class="btn-group pull-right m-1">
</div>

