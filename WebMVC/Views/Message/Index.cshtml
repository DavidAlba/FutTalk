@{
    Layout = "_MessageLayout";
}

@model MessagesListViewModel
@foreach (var msg in Model.Messages)
{
    <partial name="MessageSummary" model="msg" />
}

<div page-model-messages="@Model.PagingInfo" page-action="Index" page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-secondary"
     page-class-selected="btn-primary" page-url-category="@Model.CurrentCategory"
     class="btn-group pull-right m-1">
</div>